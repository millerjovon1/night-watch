<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>Add a movie to <%= @list.name %></h1>
      <%= simple_form_for [@list, @bookmark] do |f| %>
        <%= f.input :comment%>
        <%= f.association :movie %>
        <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
